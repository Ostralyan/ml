def cost_function(w, b, x_train, y_train):
    m = x_train.shape[0]
    cost_sum = 0
    for i in range(m):
        cost_sum += (linear_regression_model(w, x_train[i], b) - y_train[i]) ** 2
    return cost_sum / (2 * m)

